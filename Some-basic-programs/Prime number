/* program to find prime number */

#include<stdio.h>
int main()
{
  printf("enter a number\n");
  scanf("%d",&n);
  for(i=2;i<n/2;i++)
  {
    if(n%i==0)
     {
      printf("%d is not a prime numnber",n);
      exit(0);
     }
    else
      { 
        printf("%d is a prime numnber",n);
       }
   }
}
